<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.maitan.projet">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.maitan.projet.tela.Principal">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.maitan.projet.tela.CadastroLocacao" />
        <activity android:name="com.example.maitan.projet.tela.EditarLocacao" />
        <activity android:name="com.example.maitan.projet.tela.ListarLocacao" />
        <activity android:name="com.example.maitan.projet.tela.EncerrarLocacao" />

    </application>

</manifest>